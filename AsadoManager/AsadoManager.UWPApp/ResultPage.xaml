<Page
    x:Class="AsadoManager.UWPApp.ResultPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AsadoManager.UWPApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.BottomAppBar>
        <CommandBar>
            <CommandBar.Content>
                <Grid/>
            </CommandBar.Content>
            <AppBarButton Icon="Back" Label="Volver" Click="AppBarButton_Click"/>
        </CommandBar>
    </Page.BottomAppBar>

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel x:Name="stackPanel" HorizontalAlignment="Left" Height="415" Margin="10,77,0,0" VerticalAlignment="Top" Width="340" BorderBrush="#FF616161" Background="#FF2B2B2B" />
        <TextBlock x:Name="textBlockTitleRes" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="RESULTADO"  VerticalAlignment="Top" Width="233" FontWeight="Bold"/>

    </Grid>
</Page>
